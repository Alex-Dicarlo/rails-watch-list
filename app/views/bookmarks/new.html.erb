<div class="movie-container">
<div class="container w-50 mt-10" >
<h1> New Movie </h1>
<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.association :movie %>
  <%= f.input :comment %>
  <%= f.submit "Submit Movie", class: "btn btn-primary" %>
<% end %>
</div>
</div>
<p style="text-align: center"><%= link_to "Main Page", lists_path, class: "btn btn-primary" %></p>
